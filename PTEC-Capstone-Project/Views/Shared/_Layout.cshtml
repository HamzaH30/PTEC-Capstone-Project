<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Home - TeamUP</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link
      rel="stylesheet"
      href="~/PTEC_Capstone_Project.styles.css"
      asp-append-version="true"
    />

    <!-- Font Awesome -->
    <script
      src="https://kit.fontawesome.com/b998453c7b.js"
      crossorigin="anonymous"
    ></script>
  </head>
  <body>
    <partial name="__Header" />

    <div>
      <main class="container" role="main" class="pb-3">@RenderBody()</main>
    </div>

    <partial name="__Footer" />

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
  </body>
</html>
